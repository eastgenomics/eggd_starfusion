{
  "name": "starfusion_v1.0.0(1.9)",
  "title": "STAR-Fusion v1.0.0",
  "summary": "STAR-Fusion docker",
  "properties": {
    "github release": "pending"
  },
  "dxapi": "1.0.0",
  "inputSpec": [
    {
      "name": "left_fq",
      "label": "left_fq",
      "help": "gzipped fastq for R1",
      "class": "file",
      "patterns": ["*.fastq.gz"],
      "optional": false
    },
    {
      "name": "right_fq",
      "label": "right_fq",
      "help": "gzipped fastq for R2",
      "class": "file",
      "patterns": ["*.fastq.gz"],
      "optional": false
    },
    {
      "name": "genome_lib",
      "label": "genome_lib",
      "help": "STAR genome resources",
      "class": "file",
      "optional": false
    }
  ],
  "outputSpec": [
    {
      "name": "starfusion_outputs",
      "label": "starfusion_outputs",
      "help": "STAR-Fusion output files",
      "class": "array:file"
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 6
      }
    },
    "release": "16.04",
    "file": "src/code.sh",
    "version": "0",
    "interpreter": "bash",
    "distribution": "Ubuntu"
  },
  "access": {
    "allProjects": "UPLOAD",
    "network": [
      "*"
    ]
  },
  "ignoreReuse": false,
  "regionalOptions": {
    "aws:eu-central-1": {
      "systemRequirements": {
        "main": {
          "instanceType": "mem3_ssd1_x16"
        }
      }
    }
  }
}
