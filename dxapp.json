{
  "name": "eggd_starfusion",
  "title": "eggd_starfusion",
  "summary": "Runs STAR-Fusion on aligned RNA data, and returns information about gene fusions.",
  "dxapi": "1.0.0",
  "version": "1.0.0",
  "inputSpec": [
    {
      "name": "junctions",
      "label": "STAR-output junctions file",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "sentieon_tar",
      "label": "sentieon_tar",
      "help": "tarballed sentieon download",
      "class": "file",
      "patterns": ["sentieon-genomics-*.tar.gz"],
      "optional": false
      }
  ],
  "outputSpec": [
    {
      "name": "outdir",
      "label": "outdir",
      "class": "string",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 4
      }
    },
    "interpreter": "bash",
    "file": "src/code.sh",
    "distribution": "Ubuntu",
    "release": "20.04",
    "version": "0"
  },
  "access": {
    "network": ["*"],
    "allProjects": "VIEW" 
  },
  "developers":[
    "org-emee_1"
  ],
  "authorizedUsers": [
    "org-emee_1"
  ],
  "regionalOptions": {
    "aws:eu-central-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_v2_x4"
        }
      }
    }
  }
}
